{
  "name": "@hadeshe93/webpack5-starter-vue3-ts",
  "version": "0.1.0",
  "scripts": {
    "debug": "node --inspect-brk ./node_modules/.bin/webpack-cli -- --config=./configs/webpack.config.js",
    "dev": "cross-env MODE=development TARGET_PAGE=demo1 webpack serve --config=./configs/webpack.config.js",
    "build": "rimraf dist/ && npm run build:dll && npm run build:page",
    "build:page": "cross-env MODE=production webpack --config=./configs/webpack.config.js",
    "build:dll": "cross-env MODE=production webpack --config=./configs/webpack.dll.config.js"
  },
  "main": "index.js",
  "author": "hadeshe",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "vue": "^3.2.33",
    "vue-loader": "^17.0.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime-corejs3": "^7.17.9",
    "add-asset-html-webpack-plugin": "^5.0.2",
    "core-js": "^3.22.0",
    "vue-router": "^4.1.1"
  }
}
