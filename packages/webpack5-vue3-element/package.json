{
  "name": "@hadeshe93/webpack5-vue3-element",
  "version": "0.1.0",
  "scripts": {
    "start": "vice dev",
    "build": "vice build",
    "debug": "node --inspect-brk ./node_modules/.bin/webpack-cli -- --config=./configs/webpack.config.js",
    "dev": "cross-env MODE=development TARGET_PAGE=demo1 webpack serve --config=./configs/webpack.config.js",
    "buildbackup": "rimraf dist/ && npm run build:dll && npm run build:page",
    "build:page": "cross-env MODE=production TARGET_PAGE=demo1 webpack --config=./configs/webpack.config.js",
    "build:dll": "cross-env MODE=production webpack --config=./configs/webpack.dll.config.js",
    "preview": "npx http-server ./dist"
  },
  "main": "index.js",
  "author": "hadeshe",
  "license": "MIT",
  "devDependencies": {
    "@hadeshe93/builder-webpack": "^0.1.12",
    "@hadeshe93/wpconfig-mw-vue3": "^0.1.11",
    "cross-env": "^7.0.3",
    "http-proxy": "^1.18.1",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-vue-components": "^0.22.4"
  },
  "dependencies": {
    "core-js": "^3.22.0",
    "element-plus": "^2.2.12",
    "import-html-entry": "^1.12.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.16",
    "qiankun": "^2.7.4",
    "rimraf": "^3.0.2",
    "vue": "^3.2.39",
    "vue-router": "^4.1.1",
    "wujie": "1.0.0-rc.10",
    "wujie-vue3": "1.0.0-rc.10"
  },
  "peerDependencies": {
    "vue-loader": "^16.8.3"
  }
}
